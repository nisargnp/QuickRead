{
	"manifest_version": 2,

	"name": "QuickRead",
	"description": "Generate Article Summaries - CapitalOne Challange 2016 - Nisarg Patel",
	"version": "1.0",

	"icons": {"16": "icon.png"},

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "QuickRead"
	},

	"background": {
		"scripts": ["jquery/jquery-2.2.2.js", "background.js"]
	},

	"permissions": [
		"contextMenus",
		"storage",
		"background",
		"tabs",
		"http://*/*",
		"https://*/*"
	]

}

